.body {
    width: 96%;
    height: auto;
    padding: 2%;
    display: flex;
    flex-direction: column;
    align-items: center;
    background: rgb(233, 233, 233);
    font-size: 16px;
}

.groupHeader {
    width: 96%;
    padding: 5rpx;
    border: 1px solid rgb(179, 178, 178);
    border-radius: 5%;
}
.myAddress {
    font-size: 12px;
}
.groupIntroduce{
    width: 100%;
    height:auto;
}
.introduceTop{
    display: flex;
    justify-content: space-between;
}
.introduceTop image{
    width: 80px;
    height: 80px;
}
.topRight{
    display: flex;
    flex-direction: column;
}
.introduceBottom{
    display: flex;
    flex-direction: column;
}
.groupMap{
    width: 104%;
    height: 300rpx;
    background: red;
}











































